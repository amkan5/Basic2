package service;
import java.util.List;
import domain.SubjectBean;
public interface SubjectService {
	public void createSubject(SubjectBean member);
	public List<SubjectBean> list();
	public List<SubjectBean> selectSome(String word);
	public SubjectBean selectOne(String word);
	public int countService();
	public void updateSubject(SubjectBean member);
	public void deleteSubject(SubjectBean member);
}
